{
    "name": "@cloudflare/zkp-ecdsa",
    "version": "0.2.6",
    "description": "zkp-ecdsa: A Typescript Implementation of ZKAttest",
    "repository": {
        "type": "git",
        "url": "https://github.com/cloudflare/zkp-ecdsa.git"
    },
    "contributors": [
        "Armando Faz <armfazh@cloudflare.com>",
        "Watson Ladd <watson@cloudflare.com>"
    ],
    "keywords": [
        "ecdsa",
        "zero-knowledge",
        "cryptography",
        "crypto"
    ],
    "type": "module",
    "main": "./lib/src/index.js",
    "module": "./lib/src/index.js",
    "types": "./lib/src/index.d.ts",
    "files": [
        "lib/src/!(*.tsbuildinfo)",
        "tsconfig.json"
    ],
    "license": "Apache-2.0",
    "private": false,
    "engines": {
        "node": ">=24"
    },
    "devDependencies": {
        "@eslint/eslintrc": "3.3.3",
        "@eslint/js": "9.39.3",
        "@types/benchmark": "2.1.5",
        "@typescript-eslint/eslint-plugin": "8.56.0",
        "@typescript-eslint/parser": "8.56.0",
        "0x": "6.0.0",
        "benchmark": "2.1.4",
        "eslint": "9.39.3",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-prettier": "5.5.5",
        "eslint-plugin-security": "4.0.0",
        "globals": "17.3.0",
        "prettier": "3.8.1",
        "pvutils": "1.1.5",
        "typescript": "5.9.3",
        "webpack-cli": "4.10.0"
    },
    "scripts": {
        "build": "tsc -b",
        "prepare": "tsc -b",
        "test": "tsc -b test && node ./lib/test/all.test.js",
        "example": "tsc -b example && node ./lib/example/usage.js",
        "bench": "tsc -b bench && node ./lib/bench/all.bench.js",
        "bench:hrtime": "tsc -b bench && node ./lib/bench/hrtime.js",
        "lint": "eslint . --ext .ts",
        "lint:fix": "npm run lint -- --fix",
        "flame": "tsc -b bench && 0x --output-dir flames ./lib/bench/flamegraph.js",
        "format": "prettier './(src|test|bench|example)/**/*.ts' --write",
        "bundle": "tsc -b && webpack"
    },
    "dependencies": {
        "typedjson": "1.8.0"
    },
    "overrides": {
        "d3-color": "3.1.0"
    }
}
